<?xml version="1.0" encoding="utf-8" ?>

<Patch>

    <!--Metallic walls are no longer flammible-->

    <Operation Class="PatchOperationReplace">
        <xpath>//ThingDef[defName="Steel" or defName="Plasteel" or defName="Silver" or defName="Gold"]/stuffProps/statFactors/Flammability</xpath>
        <value>
            <Flammability>0.0</Flammability>
        </value>
    </Operation>

</Patch>